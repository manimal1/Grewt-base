  "scripts": {
    "build:backend": "tsc && yarn run start:api",
    "build:backend-dev": "tsc && yarn run start:api-dev",
    "build:frontend": "webpack --env.NODE_ENV=production",
    "build:frontend-dev": "webpack-dev-server --open --env.NODE_ENV=development",
    "build": "rimraf dist && concurrently \"yarn run build:backend\" \"yarn run build:frontend\"",
    "build:dev": "concurrently \"yarn run build:backend-dev\" \"yarn run build:frontend-dev\"",
    "start:api": "export NODE_ENV=production && node ./dist/index.js",
    "start:api-dev": "export NODE_ENV=development && node ./dist/index.js",
    "start:dev": "rimraf dist && nodemon --exec yarn run build:dev",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:docs": "typedoc src/server"
  },
  "lint-staged": {
    "*.ts": [
      "eslint"
    ]
  },
  "jest": {
    "verbose": true
  },
  "_moduleAliases": {
    "@constants": "dist/constants"
  }
}
